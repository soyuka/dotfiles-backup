#142sk3
kimsufi() {
http -u -b GET https://ws.ovh.com/dedicated/r2/ws.dispatcher/getAvailability2\?callback\= | jq --arg ks $1 '.answer.availability[] | select(. | contains({reference: $ks})) | .zones[] | select(.availability != "unavailable")'

}

